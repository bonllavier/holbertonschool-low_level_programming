#include<stdio.h>
#include "holberton.h"
/**
 *_strcmp - compare to strings.
 *@s1: value to be evaluate.
 *@s2: value to be evaluate.
 *Return: not.
 */
int _strcmp(char *s1, char *s2)
{
int x;
int l = 0;
int y = 0;
for (x = 0 ; *(s1 + l) != '\0'; x++)
{
y = s1[x] - s2[x];
if (y < 0)
{
return (y);
}
if(y > 0)
{
return (y);
}
else
{
return (0);
}
}
return (0);
}
