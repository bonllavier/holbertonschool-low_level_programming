#include <stdlib.h>
#include <stddef.h>
#include "3-calc.h"
/**
 *op_add - print
 *@a: string to copy.
 *@b: size.
 *Return: int.
 *
 *op_sub - print
 *@a: string to copy.
 *@b: size.
 *Return: int.
 *
 *op_mul - print
 *@a: string to copy.
 *@b: size.
 *Return: int.
 *
 *op_div - print
 *@a: string to copy.
 *@b: size.
 *Return: int.
 *
 *op_mod - print
 *@a: string to copy.
 *@b: size.
 *Return: int.
 */
int op_add(int a, int b)
{
return (a + b);
}
int op_sub(int a, int b)
{
return (a - b);
}
int op_mul(int a, int b)
{
return (a * b);
}
int op_div(int a, int b)
{
return (a / b);
}
int op_mod(int a, int b)
{
return (a % b);
}
